{
  "name": "nice-commander",
  "version": "1.2.1",
  "description": "Nice Commander",
  "main": "dist/src/index.js",
  "author": "Mohsen Azimi <me@azimi.me>",
  "repository": {
    "type": "git",
    "url": "github.com/mohsen1/nice-commander.git"
  },
  "license": "MIT",
  "scripts": {
    "preprod": "yarn prepare",
    "prod": "NODE_ENV=production nodemon --watch dist --ext js dist/examples/basic/server.js",
    "tsc-watch": "tsc --watch",
    "example-server": "DEBUG=nice-commander nodemon --inspect --delay 0.3 --ext js --watch dist dist/examples/basic/server.js",
    "cluster-server": "NODE_ENV=production pm2 start --no-daemon --watch -i 4 dist/examples/cluster/server.js",
    "start": "concurrently npm:tsc-watch npm:example-server",
    "fix": "prettier --write '**/*.{ts,tsx,yml,js,.md}' && yarn eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint": "prettier -l '**/*.{ts,tsx,yml,js,.md}' && yarn eslint . --ext .js,.jsx,.ts,.tsx",
    "prepare": "rm -rf src/ui/.next && rm -rf dist && next build src/ui && tsc",
    "test": "jest"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.28.1",
    "@blueprintjs/icons": "^3.18.0",
    "@monaco-editor/react": "^3.0.1",
    "@zeit/next-css": "^1.0.1",
    "apollo-cache": "^1.3.2",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.12",
    "apollo-link-http": "^1.5.16",
    "apollo-link-schema": "^1.2.4",
    "apollo-server-express": "^2.9.16",
    "apollo-utilities": "^1.3.2",
    "aws-sdk": "^2.605.0",
    "config": "^3.3.1",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.10.3",
    "isomorphic-unfetch": "^3.0.0",
    "linaria": "^1.3.3",
    "lodash": "^4.17.15",
    "mysql": "^2.18.1",
    "next": "^9.2.1",
    "normalize.css": "^8.0.1",
    "pretty-bytes": "^5.3.0",
    "react": "^16.12.0",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.12.0",
    "redis": "^3.0.2",
    "redlock": "^4.1.0",
    "reflect-metadata": "^0.1.13",
    "timestring": "^6.0.0",
    "type-graphql": "^1.0.0",
    "typedi": "^0.8.0",
    "typeorm-plus": "^0.1.2",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.12.1",
    "@types/config": "^0.0.36",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "@types/graphql": "^14.0.7",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "^14.0.5",
    "@types/pretty-bytes": "^5.2.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/redis": "^2.8.14",
    "@types/redlock": "^4.0.1",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^3.0.2",
    "concurrently": "^5.1.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-react": "^7.20.0",
    "jest": "^27.0.1",
    "monaco-editor": "^0.20.0",
    "nodemon": "^2.0.2",
    "pm2": "^4.4.0",
    "prettier": "^2.0.5",
    "prop-types": "^15.7.2",
    "tree-kill": "^1.2.2",
    "tslib": "^2.0.0",
    "typescript": "^4.0.2"
  },
  "engines": {
    "node": ">=10"
  }
}
