{
  "name": "nice-commander",
  "version": "0.0.8",
  "description": "Nice Commander",
  "main": "dist/src/index.js",
  "author": "Mohsen Azimi <me@azimi.me>",
  "license": "MIT",
  "scripts": {
    "preprod": "yarn build",
    "prod": "NODE_ENV=production node dist/examples/basic/server.js",
    "tsc-watch": "tsc --watch",
    "example-server": "DEBUG=nice-commander nodemon --watch dist dist/examples/basic/server.js",
    "start": "concurrently npm:tsc-watch npm:example-server",
    "fix": "prettier --write '**/*.{ts,tsx}'",
    "prepare": "rm -rf src/ui/.next && next build src/ui && tsc"
  },
  "dependencies": {
    "apollo-cache": "^1.3.2",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.12",
    "apollo-link-http": "^1.5.16",
    "apollo-link-schema": "^1.2.4",
    "apollo-server-express": "^2.9.16",
    "apollo-utilities": "^1.3.2",
    "aws-sdk": "^2.605.0",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "graphql-redis-subscriptions": "^2.1.2",
    "graphql-subscriptions": "^1.0.0",
    "graphql-tag": "^2.10.3",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.15",
    "monaco-editor": "^0.19.0",
    "mysql": "^2.18.1",
    "next": "^9.2.1",
    "prop-types": "^15.7.2",
    "react-apollo": "^3.1.3",
    "redis": "^3.0.2",
    "redlock": "^4.1.0",
