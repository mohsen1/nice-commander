    "type-graphql": "^0.17.5",
    "typeorm": "^0.2.22"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.12.1",
    "@monaco-editor/react": "^3.0.1",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "@types/graphql": "^14.0.7",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/redis": "^2.8.14",
    "@types/redlock": "^4.0.1",
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.3",
    "react": "^16.12.0",
